<VBox id="vbox" prefHeight="400" prefWidth="800"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="view.gui.MainScreenController">
   <MenuBar fx:id="menuBar" onKeyPressed="#handleKeyInput">
      <menus>
         <Menu text="Acautelamento">
            <items>
               <MenuItem text="Novo acautelamento"/>
               <MenuItem text="Inativar acautelamento"/>
               <MenuItem text="Editar acautelamento"/>
               <MenuItem text="Listar acautelamentos"/>
            </items>
         </Menu>
         <Menu text="Bem">
            <items>
               <MenuItem text="Novo bem"/>
               <MenuItem text="Remover bem"/>
               <MenuItem text="Editar bem"/>
               <MenuItem text="Listar bens"/>
            </items>
         </Menu>
         <Menu text="Local">
            <items>
               <MenuItem text="Novo local"/>
               <MenuItem text="Remover local"/>
               <MenuItem text="Editar local"/>
               <MenuItem text="Listar locais"/>
            </items>
         </Menu>
         <Menu text="Usuário">
            <items>
               <MenuItem text="Novo usuário"/>
               <MenuItem text="Remover usuário"/>
               <MenuItem text="Editar usuário"/>
               <MenuItem text="Listar usuários"/>
            </items>
         </Menu>
         <Menu text="Help">
            <items>
               <MenuItem text="Search" disable="true"/>
               <MenuItem text="Online Manual" visible="false"/>
               <SeparatorMenuItem />
               <MenuItem text="About" onAction="#handleAboutAction"/>
            </items>
         </Menu>
      </menus>
   </MenuBar>
</VBox>
